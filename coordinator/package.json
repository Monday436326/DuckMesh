{
  "name": "duckmesh-coordinator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
   "build": "tsc",
    "dev": "tsc --watch",
    "deploy": "export $(cat .env | grep -v '^#' | xargs) && npx serverless deploy",
    "deploy:prod": "export $(cat .env | grep -v '^#' | xargs) && npx serverless deploy --stage prod",
    "deploy:dev": "export $(cat .env | grep -v '^#' | xargs) && npx serverless deploy --stage dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist .serverless",
    "info": "npx serverless info",
    "logs": "npx serverless logs -f assignJob --tail",
    "remove": "npx serverless remove",
    "postinstall": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^24.3.0",
    "serverless": "^3.40.0",
    "serverless-offline": "^14.4.0",
    "serverless-typescript": "^0.0.5",
    "typescript": "^5.9.2"
  }
}
